{
  "name": "skema-core",
  "version": "0.1.0",
  "description": "Drawing-based website development overlay using tldraw",
  "author": "Justin Wu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/petersenmatthew/skema"
  },
  "homepage": "https://github.com/petersenmatthew/skema#readme",
  "keywords": [
    "react",
    "tldraw",
    "annotation",
    "drawing",
    "dom",
    "overlay",
    "development-tool"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "skema": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": {
        "types": "./dist/server.d.mts",
        "default": "./dist/server.mjs"
      },
      "require": {
        "types": "./dist/server.d.ts",
        "default": "./dist/server.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun x tsup",
    "watch": "bun x tsup --watch",
    "dev": "bun run build && bun run watch"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "tldraw": "^3.15.5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
